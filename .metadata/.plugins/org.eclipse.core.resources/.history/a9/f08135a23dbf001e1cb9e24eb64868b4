package com.car;

public class Tata {
	public void TataFeature() {
		System.out.println("common feature of all Tata cars");
		System.out.println("lag-bearer for safer cars in India.");
	}
}

class Altroz extends Tata{
	public void AltrozFeature() {
	System.out.println("Altroz Feature:");
	System.out.println("5 seats");	
	System.out.println("AC & heater");
	}
}

class Safari extends Tata{
	public void SafariFeature() {
		System.out.println("Safari Feature:");
		System.out.println("6 seater");	
		System.out.println("4 cylinder car ");
		}
	
}

