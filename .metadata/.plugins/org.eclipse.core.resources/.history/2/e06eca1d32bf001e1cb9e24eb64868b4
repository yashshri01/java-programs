package Inherit;

public class Father {
	public void getGold() {
		System.out.println("Gold");
	}
	public void getLand() {
		System.out.println("Land");
	}
	
public class Son extends Father{
	public void getCar() {
		System.out.println("Car");
	}
}

public class IncomeTax{
	
	Son sn = new Son();
	sn.getGold();
	sn.getLand();
	sn.getCar();
}
}
