package com.car;
import java.util.*;

public class Operation {
	public static void getTata() {
		Scanner sc = new Scanner(System.in);
		System.out.println("select 1 for Altroz \nselect 2 for Safari");
		int ch = sc.nextInt();
		Altroz al =new Altroz();
		Safari sa = new Safari();
		switch(ch) {
		case 1:
			al.AltrozFeature();
			break;
		case 2:
			sa.SafariFeature();
		}
	}
}
