package com.data;

public class Switch {
	public static void main(String[] args) {
	int a=5;
	switch(a) {
	case 1+4:
		System.out.println("inside case 1");
	//break;
	case 7:
		System.out.println("inside case 2");
	//break;
	default:
		System.out.println("default");
	}
	}

}
//no same or duplicate case  => case 5 , case 1+4
