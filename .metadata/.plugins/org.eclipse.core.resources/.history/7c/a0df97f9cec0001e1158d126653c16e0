package com.car;

import java.util.Scanner;

public class Operation {
	public static void getMarutiSuzuki() {
		Scanner sc = new Scanner(System.in);
		System.out.println("select 1 for Swift \nselect 2 for Balaro");
		int ch = sc.nextInt();
		Swift al =new Swift();
		Balaro sa = new Balaro();
		switch(ch) {
		case 1:
			al.getMSdealers();
			al.getFeature();
			break;
		case 2:
			sa.getMSdealers();
			sa.getFeature();
		}
	}
}
