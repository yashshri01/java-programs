package com.data;

public class Grade {
	public static void main(String[]args) {
		int m=45;
		if(m>75) {
			System.out.println("Distinction");
		}
		else if(60<m<75) {
			System.out.println("first class");
		}
		else if(50<m<65) {
			System.out.println("second class");
		}
		else if(30<m<50) {
			System.out.println("pass");
		}
		else if(m<35)) {
			System.out.println("fail");
		}
	}
}
